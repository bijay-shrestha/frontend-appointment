    "babel-eslint": "10.0.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "bootstrap": "^4.3.1",
    "bootstrap-switch-button-react": "^1.1.0",
    "chalk": "^2.4.2",
    "check-prop-types": "^1.1.2",
    "concurrently": "^5.0.2",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
