{
  "name": "@frontend-appointment/admin",
  "version": "1.0.0",
  "main": "index.js",
  "main:src": "index.js",
  "private": true,
  "dependencies": {
    "cross-env": "5.0.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@react-workspaces/react-scripts": "3.0.1-alpha-07",
    "@frontend-appointment/ui-components": "1.0.0",
    "@frontend-appointment/authentication-module": "1.0.0",
    "@frontend-appointment/ui-elements": "1.0.0",
    "@frontend-appointment/common-sass": "1.0.0",
    "@frontend-appointment/redux-module": "1.0.0",
    "@frontend-appointment/commons": "1.0.0",
    "@frontend-appointment/thunk-middleware": "1.0.0",
    "@frontend-appointment/web-resource-key-constants":"1.0.0",
