{
  "name": "@frontend-appointment/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm build/static/js/*.map && rm build/static/css/*.map ",
    "test": "react-scripts test",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy": "serve -s build -l 5000"
  },
  "dependencies": {
    "@frontend-appointment/ui-components": "1.0.0",
    "@frontend-appointment/authentication-module": "1.0.0",
    "@frontend-appointment/ui-elements": "1.0.0",
    "@frontend-appointment/common-sass-client": "1.0.0",
    "@frontend-appointment/redux-module": "1.0.0",
    "@frontend-appointment/commons": "1.0.0",
    "@frontend-appointment/thunk-middleware": "1.0.0",
    "@frontend-appointment/web-resource-key-constants":"1.0.0",
    "@frontend-appointment/helpers": "1.0.0",
    "@react-workspaces/react-scripts": "3.0.1-alpha-07",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "cross-env": "6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.{js,jsx}",
      "!src/index.{js,jsx}",
      "!src/serviceWorker.{js,jsx}",
      "!src/**/*.(spec|test|stories).{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 98,
        "functions": 98,
        "lines": 98
      }
    }
  }
}
