{
  "name": "root",
  "private": true,
  "scripts": {
    "test": "FORCE_COLOR=true && CI=true FORCE_COLOR=true lerna run test -- --coverage",
    "deploy": "FORCE_COLOR=true lerna run deploy",
    "storybook": "cd packages/storybook && yarn storybook",
    "build-storybook": "cd packages/storybook && build-storybook",
    "build:all": "concurrently --kill-others-on-fail \"yarn workspace @frontend-appointment/admin build\"",
    "build:admin": "GENERATE_SOURCEMAP=false && yarn workspace @frontend-appointment/admin build",
    "build:client": "GENERATE_SOURCEMAP=false && yarn workspace @frontend-appointment/client build",
    "clean": "lerna exec -- rimraf dist && lerna exec -- rimraf build && lerna clean --yes && rimraf node_modules",
    "admin": "yarn workspace @frontend-appointment/admin start",
    "client": "yarn workspace @frontend-appointment/client start",
    "build": "yarn build:admin && yarn build:client",
    "analyze:admin": "yarn workspace @frontend-appointment/admin analyze",
    "analyze:client": "yarn workspace @frontend-appointment/client analyze"
  },
  "workspaces": {
    "packages": [
      "packages/apps/*",
      "packages/ui-components",
      "packages/ui-elements",
      "packages/action-module",
      "packages/authentication-module",
      "packages/common-sass-client",
      "packages/common-sass",
      "packages/commons",
      "packages/core",
      "packages/redux-module",
      "packages/thunk-middleware",
      "packages/helpers",
      "packages/web-resource-key-constants"
    ],
    "nohoist": [
      "packages/apps/**/webpack-dev-server",
      "**/webpack-dev-server",
      "**/babel-loader",
      "**/babel-jest",
      "**/eslint",
      "**/jest"
    ]
  },
  "devDependencies": {
    "@ag-grid-community/all-modules": "^22.0.0",
    "@ag-grid-community/core": "^22.1.1",
    "@ag-grid-community/react": "^22.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@loadable/component": "^5.13.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.3.1",
    "bootstrap-switch-button-react": "^1.1.0",
    "chart.js": "^2.9.3",
    "check-prop-types": "^1.1.2",
    "concurrently": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "font-awesome": "^4.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-haste-map": "^26.1.0",
    "jest-prop-type-error": "^1.1.0",
    "jest-resolve": "^26.1.0",
    "lerna": "^3.22.1",
    "material-design-icons": "^3.0.1",
    "material-icons": "^0.3.1",
    "moment": "^2.24.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.2.2",
    "react-chartjs-2": "^2.9.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-datepicker": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-dom-factories": "^1.0.2",
    "react-fontawesome": "^1.7.1",
    "react-icons": "^3.10.0",
    "react-image-crop": "^8.6.4",
    "react-images": "1.1.7",
    "react-photo-gallery": "^8.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-select": "^3.0.8",
    "react-selectize": "^3.0.1",
    "react-to-print": "^2.9.0",
    "reactstrap": "^8.5.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.3",
    "secure-ls": "^1.2.6",
    "sinon": "^7.4.1",
    "source-map-explorer": "^2.4.2",
    "typescript": "^3.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.{js,jsx}",
      "!src/index.{js,jsx}",
      "!src/serviceWorker.{js,jsx}",
      "!src/**/*.(spec|test|stories).{js,jsx}"
    ]
  },
  "version": "1.0.0",
  "main": "index.js",
  "author": "Cogent Health",
  "license": "MIT",
  "production": true,
  "development": true,
  "package-entry": "main:src",
  "homepage": ".",
  "dependencies": {
    "react-loadable": "^5.5.0"
  }
}
